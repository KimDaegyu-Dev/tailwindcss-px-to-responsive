{
  "name": "tailwindcss-px-to-responsive",
  "version": "1.0.0",
  "description": "A Tailwind plugin for converting pixel values to rem, vw, vh units. It helps you to create Design-to-Development workflow easier and faster.",
  "keywords": [
    "tailwindcss",
    "tailwind",
    "rem",
    "pixel-to-rem",
    "px-to-rem",
    "px-to-vw",
    "px-to-vh",
    "pixel-to-viewport",
    "vw",
    "vh",
    "rem-converter",
    "viewport-units",
    "viewport-units-plugin",
    "viewport-converter",
    "tailwind-preset",
    "tailwind-px-to-responsive",
    "tailwind-px-to-vw",
    "tailwind-px-to-vh",
    "rem-units"
  ],
  "repository": "github:KimDaegyu-Dev/tailwindcss-px-to-responsive",
  "license": "MIT",
  "author": "KimDaegyu",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    "./useResizeHook": {
      "types": "./dist/useResizeHook.d.ts",
      "import": "./dist/useResizeHook.js",
      "require": "./dist/useResizeHook.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "release": "changeset publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "undici": "^7.11.0",
    "undici-types": "^7.11.0"
  }
}
